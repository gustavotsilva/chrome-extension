{
    "name": "Origin Employees Extension",
    "description": "This extension aims to enrich the experience of Origin Employees on Google Chrome.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "icons": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "64": "icons/64x64.png",
      "96": "icons/96x96.png",
      "128": "icons/128x128.png",
      "256": "icons/256x256.png"
    }
  }